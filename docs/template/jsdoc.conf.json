{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "systemName": "ShopIonic",
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "footer": "",
    "copyright": "Oceanhouse21 Copyright Â© 2014-2015",
    "navType": "vertical",
    "theme": "paper",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "highlightTutorialCode": true,
    "protocol": "html://",
    "default": {
      "outputSourceFiles": true
    }
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
